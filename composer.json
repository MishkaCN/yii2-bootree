{
    "name": "cn13/yii2-bootree",
    "description": "Tree ActiveRecord",
    "keywords": ["yii2", "composer", "extension installer"],
    "type": "composer-plugin",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/cn13/yii2-bootree"
    },
    "authors": [
        {
            "name": "Mikhail Narzyaev",
            "email": "m@cn13.ru"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.0"
    },
    "autoload": {
        "psr-4": { "yii\\composer\\": "" }
    },
    "extra": {
        "class": "yii\\composer\\Plugin",
        "branch-alias": {
            "dev-master": "0.0.0"
        }
    }
}
